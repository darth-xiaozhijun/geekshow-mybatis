<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.geekshow.mybatis.mapper.IUserMapper">

	<select id="selectByPrimaryKey" resultType="com.geekshow.mybatis.entity.User"
		parameterType="int">
		SELECT * FROM t_user where id = ?;
	</select>

	<select id="selectAll" resultType="com.geekshow.mybatis.entity.User">
		SELECT * FROM t_user;
	</select>

</mapper>